{
    "name": "alvarezcasarez/wykopsdk",
    "description": "Fork of xzsoftware/wykopsdk SDK dla Api v2 Wykopu",
    "type": "library",
    "keywords": ["wykop", "sdk", "wykopsdk", "wykopapi", "api"],
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "monolog/monolog": "^1.24"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.12.18",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "8.2.*",
        "povils/phpmnd": "^2.0",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Adrian Modliszewski",
            "email": "amodliszewski@xzsoftware.pl"
        }
    ],
    "minimum-stability": "stable",
    "autoload":
    {
        "psr-4":{
            "XzSoftware\\WykopSDK\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":{
            "XzSoftware\\Tests\\WykopSDK\\": "tests/"
        }
    }
}
